      <tr>
        <td>waitForElementPresent</td>
        <td>//a[text()="TEST-SLAPOSJS-PROJECT-TO-SHARE-COMPUTER 0"]</td>
        <td></td>
      </tr>
      <tr>
        <td>assertElementPresent</td>
        <td>//a[text()="TEST-SLAPOSJS-PROJECT-TO-SHARE-COMPUTER 0"]</td>
        <td></td>
      </tr>
      <tr>
        <td>click</td>
        <td>//a[text()="TEST-SLAPOSJS-PROJECT-TO-SHARE-COMPUTER 0"]</td>
        <td></td>
      </tr>

