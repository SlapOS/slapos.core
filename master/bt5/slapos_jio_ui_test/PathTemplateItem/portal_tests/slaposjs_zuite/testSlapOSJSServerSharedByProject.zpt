        <td>waitForTextPresent</td>
        <td>TEST-SLAPOSJS-PROJECT-TO-SHARE-COMPUTER 0</td>
        <td></td>
      </tr>
      <tr>
        <td>assertTextPresent</td>
        <td>TEST-SLAPOSJS-PROJECT-TO-SHARE-COMPUTER 0</td>
        <td></td>
      </tr>

